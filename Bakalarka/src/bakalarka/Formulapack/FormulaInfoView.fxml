<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>


<VBox fx:id="mainBox" alignment="TOP_CENTER" minHeight="-Infinity" minWidth="0.0" prefHeight="400.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="labelFormula" text="Formula">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <Separator prefWidth="200.0" />
      <ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" VBox.vgrow="ALWAYS">
         <content>
            <VBox alignment="TOP_CENTER">
               <children>
                  <TextFlow fx:id="tfFormulaInfo" />
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</VBox>
